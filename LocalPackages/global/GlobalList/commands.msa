
*:'/list-all' [$] = >>>
#	console('global.list_all.'.strip_colors(player()) . ' = ' . get_value('global.list_all.'.strip_colors(player())))
	if(!_cooldown(get_value('global.list_all.'.strip_colors(player())), 30)){
		die(color(RED).'Wait 30 seconds before running that again.')
	}
	if(player() != '~console'){ 
#		console('storing ' . 'global.list_all.'.strip_colors(player()) . ' = ' . time())
		store_value('global.list_all.'.strip_colors(player()), time())
	}
	@source = import('sharedata.source')
	msg(color(GREEN).'On '.@source.':')
	call_alias('/list')
	_store_shared_value(array(
		filter: 'dolist',
		data: array(
			player: player(),
		)
	))
<<<
